{
  "name": "OpenAI Server",
  "uniqueId": "openai_server",
  "description": "Connects to OpenAI's API to run hosted models like GPT-4 or GPT-3.5 via FastChat.",
  "plugin-format": "python",
  "type": "loader",
  "version": "0.1.1",
  "model_architectures": ["LlamaForCausalLM"],
  "files": ["main.py", "setup.sh"],
  "setup-script": "setup.sh",
  "parameters": {
    "model_name": {
    "title": "Model Name",
    "type": "string",
    "required": true,
    "default": "gpt-3.5-turbo"
  }
  },
  "supported_hardware_architectures": ["cpu"]
}
