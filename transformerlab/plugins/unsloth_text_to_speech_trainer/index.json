{
  "name": "Unsloth Text-to-Speech Trainer",
  "uniqueId": "unsloth_text_to_speech_trainer",
  "description": "A Text-to-Speech (TTS) trainer based on the unsloth audio training notebooks",
  "plugin-format": "python",
  "type": "trainer",
  "version": "0.0.3",
  "model_architectures": [
    "CsmForConditionalGeneration",
    "LlamaForCausalLM"
  ],
  "supports": [
  ],
  "supported_hardware_architectures": ["cuda", "amd"],
  "git": "",
  "url": "",
  "files": ["main.py", "setup.sh"],
  "setup-script": "setup.sh",
  "training_template_format": "none",
  "parameters": {
    "sampling_rate": {
      "title": "Sampling Rate",
      "type": "integer",
      "default": 24000
    },
    "maximum_sequence_length": {
      "title": "Maximum Sequence Length",
      "type": "integer",
      "default": 1024,
      "minimum": 1
    },
    "batch_size": {
      "title": "Batch Size",
      "type": "integer",
      "default": 1,
      "minimum": 1
    },
    "learning_rate_schedule": {
      "title": "Learning Rate Schedule",
      "type": "string",
      "enum": ["constant", "linear", "cosine", "constant_with_warmup"],
      "default": "linear"
    },
    "learning_rate": {
      "title": "Learning Rate",
      "type": "number",
      "default": 5e-5,
      "minimum": 1e-6
    },
    "num_train_epochs": {
      "title": "Number of Training Epochs",
      "type": "integer",
      "default": 1,
      "minimum": 1
    },
    "max_steps": {
      "title": "Max Steps (-1 means no limit)",
      "type": "integer",
      "default": -1
    },
    "lora_r": {
      "title": "Lora R",
      "type": "number",
      "minimum": 4,
      "maximum": 64,
      "multipleOf": 4,
      "default": 16
    },
    "lora_alpha": {
      "title": "Lora Alpha",
      "type": "number",
      "minimum": 4,
      "maximum": 128,
      "multipleOf": 4,
      "default": 32
    },
    "lora_dropout": {
      "title": "Lora Dropout",
      "type": "number",
      "minimum": 0.0,
      "maximum": 0.9,
      "default": 0.0
    },
    "max_grad_norm": {
      "title": "Max Grad Norm",
      "type": "number",
      "minimum": 0.0,
      "maximum": 1.0,
      "default": 0.3
    },
    "weight_decay": {
      "title": "Weight Decay",
      "type": "number",
      "minimum": 0.0,
      "maximum": 1.0,
      "default": 0.0
    },
    "adam_beta1": {
      "title": "Adam Beta 1",
      "type": "number",
      "minimum": 0.0,
      "maximum": 1.0,
      "default": 0.9
    },
    "adam_beta2": {
      "title": "Adam Beta 2",
      "type": "number",
      "minimum": 0.0,
      "maximum": 1.0,
      "default": 0.999
    },
    "adam_epsilon": {
      "title": "Adam Epsilon",
      "type": "number",
      "minimum": 0.0,
      "maximum": 1.0,
      "default": 0.00000001
    },
    "adaptor_name": {
      "title": "Adaptor Name",
      "type": "string",
      "required": true,
      "default": "tts-lora-adapter"
    },
    "audio_column_name": {
      "title": "Audio Column Name",
      "type": "string",
      "required": true,
      "default": "audio"
    },
    "text_column_name": {
      "title": "Text Column Name",
      "type": "string",
      "required": true,
      "default": "text"
    },
    "log_to_wandb": {
      "title": "Log to Weights and Biases",
      "type": "boolean",
      "default": true,
      "required": true
    }
  },
  "parameters_ui": {
    "maximum_sequence_length": {
      "ui:help": "Maximum sequence length for the model. Longer sequences will be truncated. Keep lower to save memory."
    },
    "batch_size": {
      "ui:help": "The number of sequences processed simultaneously during training. Higher values lower number of iterations but require more memory."
    },
    "lora_r": {
      "ui:widget": "range",
      "ui:help": "Rank of the update matrices, expressed in int. Lower rank results in smaller update matrices with fewer trainable parameters."
    },
    "lora_alpha": {
      "ui:widget": "range",
      "ui:help": "LoRA scaling factor. Make it a multiple of LoRA R."
    },
    "audio_column_name": {
      "ui:help": "Name of the column in your dataset that contains the audio data."
    },
    "text_column_name": {
      "ui:help": "Name of the column in your dataset that contains the text transcriptions."
    },
    "log_to_wandb": {
      "ui:help": "Log training to Weights and Biases. You must have a Weights and Biases account and API key to use this feature. You need to set the API Key in settings to use this feature."
    }
  }
}
